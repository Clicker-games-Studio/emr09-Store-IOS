<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>items</key>
    <array>
      <dict>
        <key>assets</key>
        <array>
          <!-- Update with your IPA file URL -->
          <dict>
            <key>kind</key>
            <string>software-package</string>
            <key>url</key>
            <string>https://yourserver.com/path/to/yourapp.ipa</string> <!-- Change this to your actual IPA URL -->
          </dict>
          <!-- Update with your display image URL -->
          <dict>
            <key>kind</key>
            <string>display-image</string>
            <key>url</key>
            <string>https://clicker-games.netlify.app/logo.png</string> <!-- Change this to your actual image URL -->
          </dict>
          <!-- Update with your full-size image URL -->
          <dict>
            <key>kind</key>
            <string>full-size-image</string>
            <key>url</key>
            <string>https://clicker-games.netlify.app/logo.png</string> <!-- Same as display image -->
          </dict>
        </array>
        <key>metadata</key>
        <dict>
          <!-- Update with your bundle identifier -->
          <key>bundle-identifier</key>
          <string>org.emir.flappybird</string> <!-- Change this to your app's identifier -->
          <!-- Update with your app version -->
          <key>bundle-version</key>
          <string>3.8</string> <!-- Change this to your app's version -->
          <!-- The type of asset (software) -->
          <key>kind</key>
          <string>software</string>
          <!-- Platform for iOS -->
          <key>platform-identifier</key>
          <string>com.apple.platform.iphoneos</string>
          <!-- The name of your app -->
          <key>title</key>
          <string>Flappy Bird SIGNED BY emr09</string> <!-- Change this to your app's name -->
        </dict>
      </dict>
    </array>
  </dict>
</plist>
